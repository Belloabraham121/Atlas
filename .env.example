# Hedera network (testnet or mainnet)
HEDERA_NETWORK=testnet

# Optional operator credentials (needed for contract interactions)
HEDERA_OPERATOR_ID=0.0.xxxxx
HEDERA_OPERATOR_KEY=302e020100300506032b657004220420...

# App port
PORT=3000

# Enable/disable X (Twitter) data integration (true/false)
ENABLE_X_DATA=true

# X (Twitter) API v2 bearer token
X_BEARER_TOKEN=YOUR_TWITTER_BEARER_TOKEN

# NewsAPI key
NEWSAPI_KEY=YOUR_NEWSAPI_KEY

# OpenAI API key for Hedera Agent Kit
OPENAI_API_KEY=YOUR_OPENAI_API_KEY